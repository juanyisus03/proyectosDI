<Page x:Class="EjemploFrame.Pages.PaginaInicio"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:local="clr-namespace:EjemploFrame.Pages"
    mc:Ignorable="d" 
    d:DesignHeight="500" d:DesignWidth="500"
    Title="PaginaInicio">

    <Grid>
        <StackPanel>
            <Label Content="MENU PRINCIPAL APLICACION"
                FontSize="16"
                FontWeight="Black"/>
            <Button
                x:Name="btnClientes"
                Content="Clientes"
                Width="100"
                Height="20" Click="btnClientes_Click"/>
            <Button
                x:Name="btnPedidos"
                Content="Pedidos"
                Width="100"
                Height="20" Click="btnPedidos_Click"/>

        </StackPanel>
    </Grid>
</Page>
